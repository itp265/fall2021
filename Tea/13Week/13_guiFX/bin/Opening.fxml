<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="191.0" prefWidth="432.0" spacing="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OpeningScene">
   <children>
      <Label text="Person Data">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <HBox prefHeight="40.0" prefWidth="422.0" spacing="10.0">
         <children>
            <Label text="First Name " />
            <TextField fx:id="fNameFiled" />
         </children>
      </HBox>
      <HBox prefHeight="46.0" prefWidth="422.0" spacing="10.0">
         <children>
            <Label text="Last Name" />
            <TextField fx:id="lnameField" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label text="Birthday" />
            <DatePicker fx:id="datePicker" prefHeight="25.0" prefWidth="206.0" />
         </children>
      </HBox>
      <Button fx:id="SubmitButton" mnemonicParsing="false" onAction="#doSubmitData" text="Submit" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
